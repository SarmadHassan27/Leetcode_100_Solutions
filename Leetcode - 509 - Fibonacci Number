class Solution {
    public int fib(int n) {
        if(n==0){
            return 0;
        }
        if(n==1){
            return 1;
        }

        int firstterm=0;
        int secondterm=1;


    for(int i=0;i<n;i++){

       int   thirdterm = firstterm + secondterm;

firstterm=secondterm;
secondterm=thirdterm;

    }
    // return thirdterm ;
   return firstterm;
    }
}
